<UserControl
    x:Class="DimensionForge._3D.Views.ToolbarViews.Edit3DObjectsView"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:local="clr-namespace:DimensionForge._3D.Views.ToolbarViews"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    xmlns:vs="clr-namespace:DimensionForge._3D.ViewModels"
    d:DesignHeight="450"
    d:DesignWidth="800"
    d:DataContext="{d:DesignInstance Type=vs:Edit3DObjectsViewModel}"
    mc:Ignorable="d">
    <Grid>

        <StackPanel Margin="5" Orientation="Horizontal">


            <Button Margin="5" Command="{Binding RandomCylinderCommand}">random Cyl</Button>
            <Button Margin="5" Command="{Binding RandomSpereCommand}">random Sphere</Button>
            <Button Margin="5" Command="{Binding ImportCommand}">Import OBJ</Button>
          
        </StackPanel>




    </Grid>
</UserControl>
